[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[user]
  email = hezbymuhammad@gmail.com
  name = hezbymuhammad
[push]
  default = current
[color]
  ui = auto
[merge]
  ff = only
[commit]
  template = ~/.gitmessage
[fetch]
  prune = true
[rebase]
  autosquash = true
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[diff]
  colorMoved = zebra
[alias]
  bb = !better-git-branch.sh
